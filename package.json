{
    "name": "react-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "antd-mobile": "^2.3.4",
        "axios": "^0.21.0",
        "classnames": "^2.2.6",
        "fastclick": "^1.0.6",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-redux": "^7.2.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.1",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "web-vitals": "^0.2.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "plugins": [
            "react",
            "react-hooks"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended",
            "react-app",
            "prettier",
            "prettier/@typescript-eslint",
            "prettier/react"
        ],
        "rules": {
            "react-hooks/rules-of-hooks": "error",
            "react-hooks/exhaustive-deps": "warn"
        }
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "printWidth": 120,
        "semi": true,
        "singleQuote": true,
        "jsxSingleQuote": true
    },
    "browserslist": [
        "Android >= 51",
        "ChromeAndroid >= 51",
        "ios >= 10"
    ],
    "devDependencies": {
        "@types/fastclick": "^1.0.29",
        "@types/node": "^14.14.10",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-redux": "^7.1.11",
        "@types/react-router-dom": "^5.1.6",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0",
        "babel-plugin-import": "^1.13.3",
        "prettier": "^2.2.1",
        "redux-devtools-extension": "^2.13.8",
        "react-hot-loader": "^4.13.0",
        "redux-logger": "^3.0.6",
        "sass": "^1.29.0",
        "sass-loader": "^10.1.0",
        "less": "^3.12.2",
        "less-loader": "^7.1.0",
        "typescript": "^4.1.2"
    }
}
